    _recordBtn.hidden = YES;
    [self addSubview:_recordBtn];
    [_recordBtn mas_makeConstraints:^(MASConstraintMaker *make) {
        make.centerY.height.mas_equalTo(_titleLabel);
        make.right.mas_offset(-w750(30));
    }];
    
